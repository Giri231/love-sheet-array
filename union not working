#include<bits/stdc++.h>

using namespace std;

int main()
{
    int m,n;
    cin>>m>>n;
    int a[m],b[n];
    unordered_map<int,int>map;
    for(int i=0;i<m;i++)
    {
        cin>>a[i];
        map[a[i]]++;
        
    }
    for(int i=0;i<n;i++)
    {
        cin>>b[i];
        map[b[i]]++;
        
        
    }
    cout<<map.size()<<endl;
    
}
