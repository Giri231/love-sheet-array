#include<iostream>
using namespace std;
int main()
{
    int m,n;
    cin>>m>>n;
    int a[m],b[n];
    for(int i=0;i<m;i++)
    {
        cin>>a[i];
    }
   for(int i=0;i<n;i++)
   {
       cin>>b[i];
   }
   int x=0;
   int n1=m+n;
   int c[n1];
   for(int i=0;i<m;i++)
   {
       c[x]=a[i];
       x++;
   }
   for(int i=0;i<n;i++)
   {
       c[x]=b[i];
       x++;
   }
   for(int i=0;i<n1;i++)
   {
       cout<<c[i];
   }
}
