#include<iostream>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
        
    }
    int c[n];
    int x=0;
    // cin>>k;
   for(int i=0;i<n;i++)
   {
       
           if(a[i]<0)
           {
            c[x]=a[i];
            x++;
           }
           
   }
    for(int i=0;i<n;i++)
    {
           if(a[i]>=0)
           {
               c[x]=a[i];
               x++;
           }
       
   }
    for(int i=0;i<n;i++)
    {
        a[i]=c[i];
        
    }
    for(int i=0;i<n;i++)
    {
        cout<<a[i];
    }
      /* cout<<"minimum "<<a[k-1]<<" "<<"max"<< a[n-k];*/
      
}
